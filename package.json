{
  "name": "Hotels-server",
  "version": "0.0.0",
  "description": "Hotels server",
  "author": "",
  "license": "MIT",
  "scripts": {
    "format": "tslint --fix './src/**/*.ts'",
    "start": "node server.js",
    "start:dev": "set NODE_ENV=dev&& nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && tsc",
    "start:prod": "set NODE_ENV=prod&& node dist/main.js",
    "start:hmr": "node dist/server",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "webpack": "webpack --config webpack.config.js",
    "postinstall": "npm run webpack"
  },
  "dependencies": {
    "@nestjs/common": "^5.3.7",
    "@nestjs/core": "^5.3.7",
    "@nestjs/jwt": "^0.1.3",
    "@nestjs/passport": "^5.1.0",
    "bcrypt": "^2.0.1",
    "class-transformer": "^0.1.10",
    "cloudinary": "^1.11.0",
    "dotenv": "^6.0.0",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.10",
    "multer-storage-cloudinary": "^2.2.1",
    "nodemailer": "^4.6.8",
    "passport": "^0.4.0",
    "passport-github": "^1.1.0",
    "passport-google-oauth": "^1.0.0",
    "passport-jwt": "^4.0.0",
    "passport-linkedin-oauth2": "^1.5.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "pg": "^7.4.3",
    "pug": "^2.0.3",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.3.2",
    "sequelize": "^4.38.0",
    "sequelize-typescript": "^0.6.6",
    "typescript": "^3.0.1",
    "uuid": "^3.3.2",
    "xml2js": "^0.4.19",
    "xmlbuilder": "^10.1.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.1.0",
    "@types/es6-shim": "^0.31.37",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.7.1",
    "@types/passport": "^0.4.6",
    "@types/supertest": "^2.0.5",
    "jest": "^23.5.0",
    "nodemon": "^1.18.3",
    "rimraf": "^2.6.2",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "tslint": "5.11.0",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": "10.9.0",
    "npm": "6.4.1"
  }
}
